    <link href="~/content/custom.css" rel="stylesheet">

    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <script type="text/javascript">

        function addLoad() {
            $body = $("body");
            $body.addClass("loading");
        }

        function closeLoad() {
            $body = $("body");
            $body.removeClass("loading");
        }
    </script>




